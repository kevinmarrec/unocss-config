{
  "name": "@kevinmarrec/unocss-config",
  "type": "module",
  "version": "1.3.0",
  "packageManager": "bun@1.2.22",
  "description": "Opinionated UnoCSS config.",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/unocss-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/unocss-config.git"
  },
  "keywords": [
    "unocss",
    "unocss-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "bun run check:unused && bun run check:eslint && bun run check:types",
    "check:eslint": "eslint . --cache",
    "check:types": "tsc --noEmit",
    "check:unused": "knip -n",
    "lint": "bun run check:eslint",
    "lint:inspect": "bunx @eslint/config-inspector",
    "release": "bumpp",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "unocss": ">=66.0.0"
  },
  "dependencies": {
    "@unocss/core": "^66.5.1",
    "@unocss/preset-icons": "^66.5.1",
    "@unocss/preset-web-fonts": "^66.5.1",
    "@unocss/preset-wind4": "^66.5.1",
    "@unocss/transformer-directives": "^66.5.1",
    "@unocss/transformer-variant-group": "^66.5.1"
  },
  "devDependencies": {
    "@kevinmarrec/eslint-config": "^1.4.0",
    "@kevinmarrec/tsconfig": "^1.1.0",
    "@types/bun": "^1.2.22",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "knip": "^5.63.1",
    "pathe": "^2.0.3",
    "taze": "^19.7.0",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
